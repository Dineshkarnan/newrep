package exc1.tst;

public class Employee_Details {
	public void empid()
	{
		System.out.println("8127");
	}
	public void empname()
	{
		System.out.println("Dinesh_Karunanithi");
	}
	public void empmobile()
	{
		System.out.println("+918344210128");
	}
	public void empdob()
	{
		System.out.println("24/03/1994");
	}
	public void empaddress()
	{
		System.out.println("Porur");
	}

	public static void main(String[] args) {
		Employee_Details emp=new Employee_Details();
		emp.empid();
		emp.empname();
		emp.empmobile();
		emp.empdob();
		emp.empaddress();
		
		
	}
	
	
	
	
	
	

}
